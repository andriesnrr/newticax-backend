{
  "name": "newticax-backend",
  "version": "1.0.0",
  "description": "NewticaX Backend API for Railway Deployment",
  "main": "dist/app.js",
  "type": "commonjs",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "test": "jest",
    "dev": "tsx watch src/app.ts",
    "build": "npm run clean && npm run prisma:generate && npx tsc",
    "build:railway": "rm -rf dist && npx prisma generate && npx tsc --build",
    "compile": "npx tsc",
    "clean": "rm -rf dist",
    "start": "node dist/app.js",
    "start:railway": "npm run build:railway && npm run start",
    "setup:database": "node dist/scripts/setup-database.js",
    "prisma:generate": "npx prisma generate",
    "prisma:push": "npx prisma db push",
    "prisma:studio": "npx prisma studio",
    "prisma:migrate": "npx prisma migrate dev",
    "fix:admin": "tsx scripts/fix-admin-username.ts",
    "fix:admin:prod": "node dist/scripts/fix-admin-username.js",
    "type-check": "npx tsc --noEmit",
    "lint": "echo \"Linting not configured\"",
    "health": "curl -f http://localhost:${PORT:-4000}/health || exit 1",
    "test:connection": "node -e \"fetch('http://localhost:${PORT:-4000}/health').then(r=>r.json()).then(console.log).catch(console.error)\""
  },
  "dependencies": {
    "@prisma/client": "^6.10.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.17.18",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.19.1",
    "@types/nodemailer": "^6.4.17",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^7.2.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.2",
    "nodemailer": "^6.10.1",
    "prisma": "^6.10.1",
    "slugify": "^1.6.6",
    "typescript": "^5.8.3",
    "winston": "^3.17.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.0.3",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "express",
    "prisma",
    "mongodb",
    "news-api",
    "railway"
  ],
  "author": "NewticaX Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/andriesnrr/newticax-backend"
  },
  "packageManager": "npm@10.0.0"
}
