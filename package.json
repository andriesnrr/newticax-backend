{
  "name": "newticax-backend",
  "version": "1.0.0",
  "description": "NewticaX Backend API for Railway Deployment",
  "main": "dist/app.js",
  "type": "commonjs",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
"scripts": {
    "dev": "tsx watch src/app.ts",
    
    "build": "npm run clean && npm run prisma:generate && npm run compile",
    "build:railway": "npm run clean && npx prisma generate --no-engine && npm run compile",
    
    "compile": "npx tsc",
    "compile:check": "npx tsc --noEmit",
    "clean": "rimraf dist",
    "clean:windows": "if exist dist rmdir /s /q dist",
    "clean:unix": "rm -rf dist",
    
    "start": "node dist/app.js",
    "start:dev": "npm run build && npm start",
    
    "railway:start": "npm run setup:database && node dist/app.js",
    "railway:build": "npm run clean:unix && npx prisma generate --no-engine && npm run compile",
    
    "setup:database": "node dist/scripts/setup-database.js",
    "setup:database:dev": "tsx scripts/setup-database.ts",
    
    "prisma:generate": "npx prisma generate --no-engine",
    "prisma:push": "npx prisma db push",
    "prisma:studio": "npx prisma studio",
    "prisma:migrate": "npx prisma migrate dev",
    
    "fix:admin": "tsx scripts/fix-admin-username.ts",
    "fix:admin:prod": "node dist/scripts/fix-admin-username.js",
    
    "type-check": "npx tsc --noEmit",
    "lint": "echo \"Linting not configured\""
  },
  "dependencies": {
    "@prisma/client": "^6.10.1",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^7.2.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.2",
    "nodemailer": "^6.10.1",
    "slugify": "^1.6.6",
    "winston": "^3.17.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.17.18",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.19.1",
    "@types/nodemailer": "^6.4.17",
    "prisma": "^6.10.1",
    "rimraf": "^5.0.5",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "express",
    "prisma",
    "mongodb",
    "news-api",
    "railway"
  ],
  "author": "NewticaX Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/andriesnrr/newticax-backend"
  },
  "packageManager": "npm@10.0.0"
}